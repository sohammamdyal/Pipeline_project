pipeline{
    agent any

    stages{

        stage('compile'){

            steps{
                  bat 'javac Test.java'
            }
           
        }

        stage('run'){

            steps{
                 bat 'java Test'
            }
           
        }
    }


    post{

        always {

            bat "This will always run"
        }
    success{

            bat "build success"
    }

    failure{

        bat "build failed"
    }
}
}

